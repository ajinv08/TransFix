	public WebElement hpCoursesWeOffer;

	@FindBy(xpath="//strong/u[text()='Explore all Courses']")
	public WebElement hpExploreAllCoursesLink;
	
	//initialize
	
	public View_all_courses_from_HomePage() {
		PageFactory.initElements(drives, this);
